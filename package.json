{
  "name": "ffc-be-center",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env NODE_ENV=production nest start",
    "start:dev": "cross-env NODE_ENV=development nest start --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production node dist/main",
    "migrate:dev": "dotenv -e .env.development -- npx prisma migrate dev --preview-feature",
    "migrate:create:dev": "dotenv -e .env.development -- npx prisma migrate dev --create-only --preview-feature",
    "migrate:reset:dev": "dotenv -e .env.development -- npx prisma migrate reset --preview-feature",
    "migrate:deploy:dev": "dotenv -e .env.development -- npx prisma migrate deploy --preview-feature",
    "migrate:status:dev": "dotenv -e .env.development -- npx prisma migrate status --preview-feature",
    "migrate:resolve:dev": "dotenv -e .env.development -- npx prisma migrate resolve --preview-feature",
    "prisma:studio:dev": "dotenv -e .env.development -- npx prisma studio",
    "prisma:generate:dev": "dotenv -e .env.development -- npx prisma generate",
    "prisma:generate:watch:dev": "dotenv -e .env.development -- npx prisma generate --watch",
    "migrate:prod": "dotenv -e .env.development -- npx prisma migrate dev --preview-feature",
    "migrate:create:prod": "dotenv -e .env.production -- npx prisma migrate dev --create-only --preview-feature",
    "migrate:reset:prod": "dotenv -e .env.production -- npx prisma migrate reset --preview-feature",
    "migrate:deploy:prod": "dotenv -e .env.production -- npx prisma migrate deploy --preview-feature",
    "migrate:status:prod": "dotenv -e .env.production -- npx prisma migrate status --preview-feature",
    "migrate:resolve:prod": "dotenv -e .env.production -- npx prisma migrate resolve --preview-feature",
    "prisma:studio:prod": "dotenv -e .env.production -- npx prisma studio",
    "prisma:generate:prod": "dotenv -e .env.production -- npx prisma generate",
    "prisma:generate:watch:prod": "dotenv -e .env.production -- npx prisma generate --watch",
    "seed:dev": "dotenv -e .env.development -- npx prisma db seed",
    "seed:prod": "dotenv -e .env.production -- npx prisma db seed",
    "init-db:dev": "npm run prisma:generate:dev && npm run seed:dev",
    "init-db:prod": "npm run prisma:generate:prod && npm run seed:prod",
    "lint": "eslint \"{src,apps,libs,test,prisma}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@prisma/client": "^5.9.1",
    "@types/bcrypt": "^5.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "emailjs": "^3.8.0",
    "jsonwebtoken": "^9.0.2",
    "ms": "3.0.0-canary.1",
    "nodemailer": "^6.9.13",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@stylistic/eslint-plugin": "^1.7.2",
    "@stylistic/eslint-plugin-ts": "^1.7.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.22",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^6.0.0",
    "@types/uuid": "^9.0.8",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.21.0",
    "bcrypt": "^5.1.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-import-newlines": "^1.4.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "prisma": "^5.9.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
